{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "amd": true,
        "jest/globals": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            "error",
            "single"
        ]
    },
    "plugins": [
        "jest"
    ]
}